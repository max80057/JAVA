class Solution {
    public int climbStairs(int n) {
        if(n <= 0){
            return 0;
        }
        
        if(n ==1){
            return 1;
        }
        int[] types = new int[n];
        types[0]=1;
        types[1]=1;
        
        for(int i = 2 ; i < n ; i++ ){
            types[i] = types[i-1] + types[i-2];
        }
        return types[n-1];
    }
}
